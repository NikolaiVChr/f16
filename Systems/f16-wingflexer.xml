<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	<params>
	  <m-wing-dry-kg type="double">500</m-wing-dry-kg><!-- mass of single wing -->
	  <K type="double">250</K><!-- stiffness -->
	  <D type="double">25</D><!-- damping -->
	  <fuel-frac type="double">0.0010</fuel-frac><!-- infamous fudge factor -->
	  <upwards-stiffness-factor type="double">0.5</upwards-stiffness-factor><!-- how much less wings gets bend upwards than downwards -->
	  <body-factor type="double">0.5</body-factor><!-- how much less wings gets bend from carrying body in air than own weight when WoW, for realism leave this to 1.0 -->
	  <normal-node-lbs alias="/f16/wings/normal-lbf"/><!-- normal force, if inputing lift here please convert to normal force by normal=lift*cos(alpha) + drag*sin(alpha)  -->
	  <fuel-node-1-kg alias="/f16/wings/fuel-and-stores-kg"/><!-- total fuel and stores in both wings -->
	  <fuel-node-2-kg type="double">0</fuel-node-2-kg>
	  <fuel-node-3-kg type="double">0</fuel-node-3-kg>
	  <fuel-node-4-kg type="double">0</fuel-node-4-kg>
	  <z-fac type="double">1</z-fac><!-- animation factor for when animating degrees instead of flex shader -->
	  <min type="double">-0.5</min><!-- clips are applied before z-fac is multiplied onto z-m -->
      <max type="double">0.5</max>
      <aerotow-active type="bool">false</aerotow-active><!-- for now very simple, just a bool if towed or not -->
      <aircraft-g-force alias="/fdm/jsbsim/accelerations/a_n"/><!-- aircraft G force at center of gravity -->
	</params>
	<z-m type="double">0</z-m><!-- the output -->
</PropertyList>