    </channel>

    <channel name="sound" execrate="8">

        <fcs_function name="/engines/engine[0]/sound-n2-low-down">
            <function>
                <table>
                    <independentVar lookup="row">/engines/engine/n2</independentVar>
                    <tableData>
                          60          1.0
                          95         -1.0
                    </tableData>
                </table>
            </function>
        </fcs_function>

        <fcs_function name="/engines/engine[0]/sound-n2-vol-a">
            <function>
                <sqrt>
                    <product>
