    <!-- new AB internal sound by G-EED -->
    <afterburner>
     <name>afterburner internal</name>
     <path>Aircraft/f16/Sounds/afterburner_interior.wav</path>
     <mode>looped</mode>
     <condition>
      <property>sim/current-view/internal</property>
      <property>engines/engine/augmentation</property>
     </condition>
     <position>
      <x>4.6</x>
      <y>0.0</y>
      <z>0.0</z>
     </position>
     <volume>
      <property>engines/engine/nozzle-pos-norm</property>
      <offset>0.05</offset>
      <factor>0.80</factor>
     </volume>
    </afterburner>

  <rumble>
   <name>gear rumble</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <property>sim/current-view/internal</property>
    <or>
     <property>gear/gear[1]/wow</property>
     <property>gear/gear[2]/wow</property>
    </or>
   </condition>
   <reference-dist>30</reference-dist>
   <max-dist>100</max-dist>
   <position>
    <x>1.2</x>
    <y>0.0</y>
    <z>-0.6</z>
   </position>
   <volume>
    <property>velocities/groundspeed-kt</property>
    <factor>.004</factor>
   </volume>
  </rumble>

  <gear>
		<name>gear</name>
		<mode>in-transit</mode>
		<path>Sounds/gear-transit.wav</path>
		<property>/gear/gear/position-norm</property>
		<volume>
			<factor>0.9</factor>
		</volume>
		<pitch>
			<internal>dt_play</internal>
			<offset>1.0</offset>
			<factor>-0.001</factor>
		</pitch>
	</gear>

	<gear>
		<name>gear-lock</name>
		<path>Sounds/gear-lock.wav</path>
		<condition>
			<equals>
				<property>/gear/gear/position-norm</property>
				<value>0</value>
			</equals>
		</condition>
	</gear>

	<gear>
		<name>gear-open</name>
		<path>Sounds/gear-open.wav</path>
		<condition>
			<equals>
				<property>/gear/gear/position-norm</property>
				<value>1</value>
			</equals>
		</condition>
	</gear>

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <not>
     <property>sim/current-view/internal</property>
    </not>
    <or>
     <property>gear/gear[1]/wow</property>
     <property>gear/gear[2]/wow</property>
    </or>
   </condition>
   <reference-dist>150</reference-dist>
   <max-dist>500</max-dist>
   <position>
    <x>1.2</x>
    <y>0.0</y>
    <z>-0.6</z>
   </position>
   <volume>
    <property>velocities/speed-down-fps</property>
    <factor>0.05</factor>
   </volume>
   <volume>
    <property>velocities/airspeed-kt</property>
    <factor>0.01</factor>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>
 
  <stall>
   <name>stall</name>
   <path>Aircraft/f16/Sounds/betty/warning.wav</path>
   <mode>looped</mode>
   <type>avionics</type>
   <condition>
      <and>
        <property>sim/current-view/internal</property>
        <greater-than>
              <property>fdm/jsbsim/velocities/vtrue-kts</property>
              <value>20</value>
          </greater-than>
        <or>
          <greater-than>
              <property>fdm/jsbsim/aero/stall-hyst-norm</property>
              <value>0</value>
          </greater-than>
          <less-than>
              <property>fdm/jsbsim/aero/alpha-deg</property>
              <value>-15</value>
          </less-than>
        </or>
        <not>
          <or>
            <property>gear/gear[1]/wow</property>
            <property>gear/gear[2]/wow</property>
          </or>
        </not>
      </and>
   </condition>
  </stall>

  <stall>
   <name>stall2</name>
   <path>Aircraft/f16/Sounds/betty/caution.wav</path>
   <mode>once</mode>
   <type>avionics</type>
   <condition>
      <and><!-- eyebrow TO LDG CONFIG warning -->
          <property>sim/current-view/internal</property>
          <less-than>
            <property>controls/gear/gear-down</property>
            <value>1</value>
          </less-than>
          <less-than>
            <property>position/altitude-agl-ft</property>
            <value>10000</value>
          </less-than>
          <less-than>
            <property>velocities/airspeed-kt</property>
            <value>190</value>
          </less-than>
          <greater-than>
            <property>velocities/speed-down-fps</property>
            <value>4.17</value>
          </greater-than><!-- 250 fpm -->
      </and>
   </condition>
  </stall>

  <impact>
   <name>pullUp</name>
   <path>Aircraft/f16/Sounds/betty/pullup.wav</path>
   <mode>looped</mode>
   <type>avionics</type>
   <condition>
      <and>
          <greater-than>
              <property>instrumentation/radar/time-till-crash</property>
              <value>0</value>
          </greater-than>
          <less-than>
              <property>instrumentation/radar/time-till-crash</property>
              <value>10</value>
          </less-than>
      </and>
   </condition>
   <volume>
        <property>sim/current-view/internal</property>
        <factor>1</factor>
    </volume>
  </impact>


  <bingo>
   <name>bingo</name>
   <path>Aircraft/f16/Sounds/betty/bingo.wav</path>
   <mode>once</mode>
   <type>avionics</type>
   <condition>
        <property>f16/avionics/bingo</property>
    </condition>
   <volume>
        <property>sim/current-view/internal</property>
        <factor>1</factor>
    </volume>
  </bingo>
  

  <cannon>
   <name>cannon</name>
   <path>Sounds/gatling.wav</path>
   <condition>
        <property>fdm/jsbsim/fcs/guntrigger</property>
        <greater-than>
          <property>ai/submodels/submodel[2]/count</property>
          <value>0</value>
        </greater-than>
        <greater-than>
          <property>fdm/jsbsim/systems/hydraulics/sysb-psi</property>
          <value>1999</value>
        </greater-than>
        <property>payload/armament/fire-control/serviceable</property>
    </condition>
   <mode>looped</mode>
   <position>
    <x>-1.0</x>
    <y>0.8</y>
    <z>0.0</z>
   </position>
  </cannon>

  <voice>
      <name>Geforce-breath-vox</name>
      <path>Aircraft/f16/Sounds/Geforce.wav</path>
      <type>avionics</type>
      <condition>
          <greater-than>
              <property>/accelerations/pilot/z-accel-fps_sec</property>
              <value>-161</value>
          </greater-than>
      </condition>
      <reference-dist>2</reference-dist>
      <max-dist>4</max-dist>
      <volume>
          <property>sim/current-view/internal</property>
          <factor>0.9</factor>
      </volume>
  </voice>

  <voice>
      <name>Geforce-vox</name>
      <path>Aircraft/f16/Sounds/geforce1.wav</path>
      <type>avionics</type>
      <condition>
          <less-than>
              <property>/accelerations/pilot/z-accel-fps_sec</property>
              <value>-225</value>
          </less-than>
      </condition>
      <reference-dist>2</reference-dist>
      <max-dist>4</max-dist>
      <volume>
          <property>sim/current-view/internal</property>
          <factor>0.9</factor>
      </volume>
  </voice>

  <rain>
      <name>rain</name>
      <mode>looped</mode>
      <path>Aircraft/f16/Sounds/rain.wav</path>
      <type>avionics</type>
      <condition>
          <greater-than>
              <property>/environment/rain-norm</property>
              <value>0</value>
          </greater-than>
      </condition>
      <volume>
          <property>/environment/rain-norm</property>
          <factor>0.175</factor>
      </volume>
  </rain>

  <rain-windshield>
      <name>rain-windshield</name>
      <mode>looped</mode>
      <path>Aircraft/f16/Sounds/rain-windshield.wav</path>
      <type>avionics</type>
      <condition>
              <property>sim/current-view/internal</property>
              <greater-than>
                  <property>/environment/rain-norm</property>
                  <value>0</value>
              </greater-than>
      </condition>
      <volume>
          <property>velocities/airspeed-kt</property>
          <factor>0.0075</factor>
          <min>0.03</min>
          <max>0.40</max>
      </volume>
  </rain-windshield>

  <squeal>
        <name>touchdown squeal</name>
        <path>Sounds/squeal.wav</path>
        <condition>
            <and>
                <or>
                    <property>fdm/jsbsim/gear/unit[0]/WOW</property>
                    <property>fdm/jsbsim/gear/unit[1]/WOW</property>
                    <property>fdm/jsbsim/gear/unit[2]/WOW</property>
                </or>
                <less-than>
                    <property>velocities/vertical-speed-fps</property>
                    <value>-0.05</value>
                </less-than>
            </and>
        </condition>
        <reference-dist>150</reference-dist>
        <max-dist>500</max-dist>
        <position>
            <x>2.64670</x>
            <y>0.01</y>
            <z>-2.1</z>
        </position>
        <volume>
            <property>velocities/groundspeed-kt</property>
            <factor>0.01</factor>
            <max>1.0</max>
        </volume>
        <volume>
            <internal>dt_stop</internal>
            <factor>0.1</factor>
            <max>1.0</max>
        </volume>
        <pitch>
            <property>velocities/groundspeed-kt</property>
            <factor>0.0025</factor>
            <offset>1.2</offset>
        </pitch>
    </squeal>


    <aircraft-explode>
            <name>aircraft-explode</name>
            <path>Aircraft/f16/Sounds/aircraft-explode.wav</path>
            <mode>once</mode>
            <condition>
                <equals>
                    <property>damage/sounds/explode-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <volume>
                <factor>3</factor>
                <max>3</max>
            </volume>
        </aircraft-explode>

        <aircraft-crash>
            <name>aircraft-impact</name>
            <path>Aircraft/f16/Sounds/aircraft-crash.wav</path>
            <mode>once</mode>
            <condition>
                <equals>
                    <property>damage/sounds/crash-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <volume>
                <factor>1</factor>
            </volume>
        </aircraft-crash>

        <aircraft-water-crash>
            <name>aircraft-water-crash</name>
            <path>Aircraft/f16/Sounds/aircraft-water-crash.wav</path>
            <mode>once</mode>
            <condition>
                <equals>
                    <property>damage/sounds/water-crash-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <volume>
                <factor>1</factor>
            </volume>
        </aircraft-water-crash>

        <nearby-explode>
            <name>nearby-explode</name>
            <path>Aircraft/f16/Sounds/aircraft-explode.wav</path>
            <mode>once</mode>
            <condition>
                <equals>
                    <property>damage/sounds/nearby-explode-on</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>0.01</x>
                <y>0.01</y>
                <z>0.01</z>
            </position>
            <pitch>
                <factor>1.05</factor>
            </pitch>
            <volume>
                <factor>1</factor>
                <max>1</max>
            </volume>
        </nearby-explode>

        <lock-tone>
            <name>aim-9-signal</name>
            <mode>looped</mode>
            <type>avionics</type>
            <path>Aircraft/f16/Sounds/aim-9-growl.wav</path>
            <condition>
                <property>sim/current-view/internal</property>
                <property>payload/armament/aim-9/sound-on-off</property>
            </condition>
            <pitch>
                <property>payload/armament/aim-9/sound-volume</property>
                <factor>1.00</factor>
                <offset>1.60</offset>
                <min>0</min>
                <max>1</max>
            </pitch>
            <volume>
                <property>payload/armament/aim-9/sound-volume</property>
            </volume>
        </lock-tone>

        <lock-tone>
            <name>aim-7-signal</name>
            <mode>looped</mode>
            <type>avionics</type>
            <path>Aircraft/f16/Sounds/115hz-square.wav</path>
            <condition>
                <property>sim/current-view/internal</property>
                <property>payload/armament/aim-7/sound-on-off</property>
            </condition>
            <pitch>
                <factor>1.8</factor><!-- changes it to beep -->
                <max>2</max>
            </pitch>
            <volume>
                <property>payload/armament/aim-7/sound-volume</property>
            </volume>
        </lock-tone>
    
        <lock-tone>
            <name>aim-120-signal</name>
            <mode>looped</mode>
            <type>avionics</type>
