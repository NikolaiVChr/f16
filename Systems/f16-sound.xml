<?xml version="1.0"?>
 
<PropertyList>

 <fx>
  <canopy>
   <name>canopy</name>
   <path>Aircraft/f16/Sounds/canopy.wav</path>
   <mode>in-transit</mode>
   <property>fdm/jsbsim/fcs/canopy-pos-norm</property>
   <reference-dist>30</reference-dist>
   <max-dist>100</max-dist>
   <position>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.195</z>
   </position>
   <pitch>
    <internal>dt_start</internal>
    <offset>1.25</offset>
    <factor>-0.25</factor>
   </pitch>
   <volume>
    <max>0.5</max>
   </volume>
  </canopy>

  <canopy>
   <name>canopy_lock</name>
   <path>Aircraft/f16/Sounds/canopy_lock.wav</path>
   <mode>single</mode>
   <condition>
    <equals>
     <property>fdm/jsbsim/fcs/canopy-pos-norm</property>
     <value>0.0</value>
    </equals>
   </condition>
   <reference-dist>10</reference-dist>
   <max-dist>40</max-dist>
   <position>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.195</z>
   </position>
   <volume>
    <max>0.5</max>
   </volume>
  </canopy>

  <engine_fan>
   <name>internal fan</name>
   <path>Aircraft/f16/Sounds/engine-int.wav</path>
   <mode>looped</mode>
   <condition>
    <property>sim/current-view/internal</property>
    <greater-than>
        <property>engines/engine/n2</property>
        <value>5</value>
    </greater-than>
   </condition>
   <reference-dist>30</reference-dist>
   <max-dist>100</max-dist>
   <position>
    <x>-0.6</x>
    <y>0.0</y>
    <z>-0.37</z>
   </position>
   <orientation>
    <x>-0.98</x>
    <y>0.0</y>
    <z>0.0</z>
   </orientation>
   <pitch>
    <property>engines/engine/n1</property>
    <offset>0.6</offset>
    <factor>.00075</factor>
   </pitch>
   <volume>
    <factor>0.5</factor>
   </volume>
  </engine_fan>


<engine_fan>
   <name>internal fan 2</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <condition>
    <property>sim/current-view/internal</property>
    <greater-than>
        <property>engines/engine/n2</property>
        <value>5</value>
    </greater-than>
   </condition>
   <reference-dist>30</reference-dist>
   <max-dist>100</max-dist>
   <position>
    <x>-0.6</x>
    <y>0.0</y>
    <z>-0.37</z>
   </position>
   <orientation>
    <x>-0.98</x>
    <y>0.0</y>
    <z>0.0</z>
   </orientation>
   <pitch>
    <property>engines/engine/n1</property>
    <offset>0.6</offset>
    <factor>.00075</factor>
   </pitch>
   <volume>
    <factor>0.10</factor>
   </volume>
  </engine_fan>

  <engine_fan>
   <name>external fan</name>
   <path>Aircraft/f16/Sounds/turbojet_fan.wav</path>
   <mode>looped</mode>
   <condition>
    <greater-than>
        <property>engines/engine/n2</property>
        <value>5</value>
    </greater-than>
    <not>
     <property>sim/current-view/internal</property>
    </not>
    <less-than>
      <property>engines/engine/n1</property>
      <value>100.0</value>
    </less-than>
   </condition>
   <reference-dist>100</reference-dist>
   <max-dist>4000</max-dist>
   <position>
    <x>0.6</x>
    <y>0.01</y>
    <z>-0.37</z>
   </position>
   <orientation>
    <x>-0.98</x>
    <y>0.0</y>
    <z>-0.17</z>
    <inner-angle>60.0</inner-angle>
    <outer-angle>200.0</outer-angle>
    <outer-gain>0.65</outer-gain>
   </orientation>
   <pitch>
    <property>engines/engine/n1</property>
    <offset>1.0</offset>
    <factor>0.004</factor>
   </pitch>
   <volume>
    <property>engines/engine/n1</property>
    <offset>1.30</offset>
    <factor>-0.01</factor>
   </volume>
  </engine_fan>

  <exhaust>
   <name>turbine internal</name>
   <path>Sounds/jet.wav</path>
   <mode>looped</mode>
   <condition>
     <property>engines/engine/running</property>
     <property>sim/current-view/internal</property>
   </condition>
   <reference-dist>250</reference-dist>
   <max-dist>12500</max-dist>
   <position>
    <x>4.6</x>
    <y>0.01</y>
    <z>0.01</z>
   </position>
   <orientation>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>200.0</inner-angle>
    <outer-angle>360.0</outer-angle>
    <outer-gain>0.25</outer-gain>
   </orientation>
   <volume>
    <property>engines/engine/thrust_lb</property>
    <offset>0</offset>
    <factor>0.000044944</factor>
   </volume>
   <!--<volume>
    <property>sim/current-view/internal</property>
    <offset>1</offset>
    <factor>-0.5</factor>
   </volume>-->
   <pitch>
    <property>engines/engine/n1</property>
    <offset>0.25</offset>
    <factor>0.01</factor>
   </pitch>
  </exhaust>

  <exhaust>
   <name>turbine external</name>
   <path>Sounds/jet.wav</path>
   <mode>looped</mode>
   <condition>
     <property>engines/engine/running</property>
     <not>
      <property>sim/current-view/internal</property>
     </not>
   </condition>
   <reference-dist>250</reference-dist>
   <max-dist>12500</max-dist>
   <position>
    <x>4.6</x>
    <y>0.01</y>
    <z>0.01</z>
   </position>
   <orientation>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>200.0</inner-angle>
    <outer-angle>360.0</outer-angle>
    <outer-gain>0.25</outer-gain>
   </orientation>
   <volume>
    <property>engines/engine/thrust_lb</property>
    <offset>0</offset>
    <factor>0.000065</factor>
   </volume>
   <!--<volume>
    <property>sim/current-view/internal</property>
    <offset>1</offset>
    <factor>-0.5</factor>
   </volume>-->
   <pitch>
    <property>engines/engine/n1</property>
    <offset>0.00</offset>
    <factor>0.01</factor>
   </pitch>
  </exhaust>

  <afterburner>
   <name>afterburner</name>
   <path>Aircraft/f16/Sounds/exhaust_loop.wav</path>
   <mode>looped</mode>
   <condition>
    <not>
     <property>sim/current-view/internal</property>
    </not>
    <property>engines/engine/augmentation</property>
   </condition>
   <reference-dist>500</reference-dist>
   <max-dist>15000</max-dist>
   <position>
    <x>4.6</x>
    <y>0.01</y>
    <z>0.01</z>
   </position>
   <orientation>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>200.0</inner-angle>
    <outer-angle>360.0</outer-angle>
    <outer-gain>0.2</outer-gain>
   </orientation>
   <volume>
    <property>engines/engine/nozzle-pos-norm</property>
    <offset>0.05</offset>
    <factor>0.95</factor>
   </volume>
  </afterburner>

