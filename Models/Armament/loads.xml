   <animation>
  <type>select</type>
  <object-name>Station1_AIM-9L</object-name>
  <condition>
    <equals>
      <property>payload/armament/station/id-0-type</property>
      <value>AIM-9</value>
    </equals>
    <greater-than>
      <property>payload/armament/station/id-0-count</property>
      <value>0</value>
    </greater-than>
  </condition>
 </animation>
 
