<?xml version="1.0"?>



<PropertyList>

	<name>AIM-9L</name>
	<path>AIM-9X.ac</path>

	<effect>  
    	<inherits-from>Aircraft/f16/Models/Effects/glass/glassIR</inherits-from>
    	<object-name>aim9x_ir-sphere</object-name>
  	</effect>

	<effect>
      <inherits-from>Aircraft/f16/Models/Effects/fuselage/model-combined-stores</inherits-from>
      <parameters>
          <metallic type="float">0.80</metallic>
          <ambient-factor type="float">0.0</ambient-factor>
          <rain-enabled type="int">0</rain-enabled>
          <ambient-correction type="float">0.00</ambient-correction>
          <reflection-enabled type="int">0</reflection-enabled>
      </parameters>
      <object-name>aim9x-body</object-name>
      <object-name>aim9x-back-fins</object-name>
      <object-name>aim9x-front-fins</object-name>
      <object-name>aim9x-thrust-vectoring-fins</object-name>
      <object-name>aim9x-mounts</object-name>
  	</effect>

	<animation>
		<type>select</type>
      		<object-name>aim9x-body</object-name>
      		<object-name>aim9x-back-fins</object-name>
      		<object-name>aim9x-front-fins</object-name>
      		<object-name>aim9x-thrust-vectoring-fins</object-name>
      		<object-name>aim9x-mounts</object-name>
      		<object-name>aim9x_ir-sphere</object-name>
		<condition>
			<property>payload/armament/aim-9x/flags/msl-id-600</property>
		</condition>
	</animation>

	<particlesystem>
	   <name>smoke</name>
	   <texture>../smoke.png</texture>

	   <emissive type="bool">false</emissive>
	   <lighting type="bool">false</lighting>

	   <condition>
	         <property>payload/armament/aim-9x/flags/smoke-id-600</property>
	   </condition>

	   <attach>world</attach>

	   <placer>
	     <type>point</type> 
	   </placer>

	   <offsets>
	      <x-m>8.44</x-m>
	      <y-m>0</y-m>
	      <z-m>0</z-m>
	   </offsets>

	   <shooter>
	     <theta-min-deg>-1.5</theta-min-deg>
	     <theta-max-deg>1.5</theta-max-deg>
	     <phi-min-deg>-1.5</phi-min-deg>
	     <phi-max-deg>1.5</phi-max-deg>
	     <speed-mps>
	       <value>0</value>
	       <spread>0</spread>
	     </speed-mps>
	     <rotation-speed>
	       <x-min-deg-sec>0</x-min-deg-sec>
	       <y-min-deg-sec>0</y-min-deg-sec>
	       <z-min-deg-sec>0</z-min-deg-sec>
	       <x-max-deg-sec>0</x-max-deg-sec>
	       <y-max-deg-sec>0</y-max-deg-sec>
	       <z-max-deg-sec>0</z-max-deg-sec>
	     </rotation-speed>
	   </shooter>

	   <counter>
	     <particles-per-sec>
	       <value>250</value>
	       <spread>0</spread>
	     </particles-per-sec>
	   </counter>
	      
	   <align>billboard</align>

	   <particle>
	     <start>
	       <color>
	         <red><value>   1.0 </value></red>
	         <green><value> 1.0 </value></green>
	         <blue><value>  0.9 </value></blue>
	         <alpha><value> 0.3 </value></alpha> <!-- TODO: Does this need to be changed? -->
	       </color>
	       <size>
	         <value>0.8</value>
	       </size>
	     </start>

	     <end>
	       <color>
	         <red><value>   0.9 </value></red>
	         <green><value> 0.9 </value></green>
	         <blue><value>  0.9 </value></blue>
	         <alpha><value> 0.1 </value></alpha>
	       </color>
	       <size>
	         <value>6</value>
	       </size>
	     </end>

	     <life-sec>
	       <value>5</value>
	     </life-sec>
	        
	     <mass-kg>  0.5 </mass-kg>
	     <radius-m> 0.5 </radius-m>
	   </particle>

	   <program>
	     <fluid>               air  </fluid>
	     <gravity type="bool"> true </gravity>
	     <wind typ="bool">     true </wind>
	   </program>

	 </particlesystem>
	 
	 <model>
		<name>flaminghot</name>
		<path>AIM-9X_exhaust.xml</path>
		<offsets>
			<x-m>1.43018</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</offsets>
	</model>
	
	<animation>
      <type>select</type>
      <object-name>flaminghot</object-name>
      <condition>
           <and>
           <property>payload/armament/aim-9x/flags/smoke-id-600</property>
           <property>/sim/rendering/shaders/skydome</property>
       </and>
     </condition>
    </animation>


	 <particlesystem>
	   <name>exhaust</name>
	   <texture>../smoke.png</texture>

	   <emissive type="bool">true</emissive>
	   <lighting type="bool">false</lighting>

	   <condition>
	         <property>payload/armament/aim-9x/flags/smoke-id-600</property>
	         <not>
                 <property>/sim/rendering/shaders/skydome</property>
             </not>
	   </condition>

	   <attach>world</attach>

	   <placer>
	     <type>point</type> 
	   </placer>

	   <offsets>
	      <x-m>1.44</x-m>
	      <y-m>0</y-m>
	      <z-m>0</z-m>
	   </offsets>

	   <shooter>
	     <theta-min-deg>-1.5</theta-min-deg>
	     <theta-max-deg>1.5</theta-max-deg>
	     <phi-min-deg>-1.5</phi-min-deg>
	     <phi-max-deg>1.5</phi-max-deg>
