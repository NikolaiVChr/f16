<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
<!--
Used for non fired MK-82AIR
-->

  <path>MK-82AIR.ac</path>

  <effect>
      <inherits-from>Aircraft/f16/Models/Effects/fuselage/model-combined-fuselage</inherits-from>
      <parameters>
          <metallic type="float">0.80</metallic>
          <ambient-factor type="float">0.0</ambient-factor>
          <rain-enabled type="int">0</rain-enabled>
          <ambient-correction type="float">0.00</ambient-correction>
          <reflection-enabled type="int">0</reflection-enabled>
      </parameters>
      <object-name>MK-82AIR_Shell</object-name>
      <object-name>MK-82AIR_TailFins</object-name>
      <object-name>MK-82AIR_Canister</object-name>
      <object-name>MK-82AIR_Detonator</object-name>
      <object-name>MK-82AIR_Wiring</object-name>
      <object-name>MK-82AIR_Mounts</object-name>
  </effect>

  <animation>
    <type>select</type>
    <object-name>MK-82AIR_Chute</object-name>
    <condition>
      <equals>
        <value>0</value>
        <value>1</value>
      </equals>
    </condition>
  </animation>

</PropertyList>
