<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
<!--
Used for non fired MK-82AIR
-->

  <path>MK-82AIR.ac</path>

  <effect>
      <inherits-from>Aircraft/f16/Models/Effects/fuselage/model-combined-stores</inherits-from>
      <object-name>MK-82AIR_Shell</object-name>
      <object-name>MK-82AIR_TailFins</object-name>
      <object-name>MK-82AIR_Canister</object-name>
      <object-name>MK-82AIR_Detonator</object-name>
      <object-name>MK-82AIR_Wiring</object-name>
