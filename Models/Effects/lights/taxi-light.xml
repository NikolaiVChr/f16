<?xml version="1.0"?>

<PropertyList>
  <path>Models/Effects/procedural_light.ac</path>
<animation>
      <type>noshadow</type>
      <object-name>procedural_light</object-name>
  </animation> 
  <effect>
    <inherits-from>Aircraft/f16/Models/Effects/lights/taxi-light</inherits-from>
    <object-name>procedural_light</object-name>
  </effect>

  <animation>
    <type>select</type>
    <object-name>procedural_light</object-name>
    <condition>
      <property>sim/multiplay/generic/bool[46]</property> 
    </condition>
  </animation>

  <animation>
    <type>dist-scale</type>
    <object-name>procedural_light</object-name>
    <interpolation>
      <entry><ind>0</ind><dep>1.0</dep></entry>
      <entry><ind>4000</ind><dep>10.0</dep></entry>
    </interpolation>
  </animation>
</PropertyList>
