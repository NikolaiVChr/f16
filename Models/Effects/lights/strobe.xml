<?xml version="1.0"?>

<PropertyList>
  
  <model>
   <path>Aircraft/f16/Models/Effects/lights/strobe-light.xml</path>
   <name>glow</name>
  </model>

 <animation>
  <type>dist-scale</type>
  <object-name>glow</object-name>
  <interpolation>
   <entry><ind>0</ind><dep>1.5</dep></entry>
   <entry><ind>8000</ind><dep>40.0</dep></entry>
  </interpolation>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>glow</object-name>
  <condition>
   <equals>
        <property>sim/multiplay/generic/bool[44]</property> 
        <value>1</value>
      </equals> 
  </condition>
 </animation>
