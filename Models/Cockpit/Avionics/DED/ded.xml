        <object-name>poly.003</object-name>
        <type>material</type>
        <condition>
            <greater-than-equals>
                <property>f16/avionics/power-ufc-warm</property>
                <value>0.95</value>
            </greater-than-equals>
            <or>
              <greater-than-equals>
                  <property>fdm/jsbsim/elec/bus/emergency-ac-1</property>
