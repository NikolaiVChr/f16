  <layer>
   <name>system time</name>
   <type>text</type>
   <font>default</font>
   <point-size>4.56</point-size>
   <color>
    <red>0.63</red>
    <green>1.0</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>text-value</type>
     <property>/sim/time/gmt-string</property>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>10</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-1.5</offset>
    </transformation>
   </transformations>
  </layer>

