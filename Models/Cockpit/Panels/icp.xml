						<property>f16/avionics/rtn-seq</property>
						<value>0</value>
					</equals>
				</condition>
				<command>property-assign</command>
				<property>f16/avionics/ded-up-down</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>f16/avionics/ded-up-down</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	
	<animation>
        <type>pick</type>
        <visible>false</visible>
        <object-name>switchSquareDown</object-name>
		<action>
			<button>0</button>
			<binding>
				<condition>
					<equals>
						<property>f16/avionics/rtn-seq</property>
						<value>0</value>
					</equals>
				</condition>
				<command>property-assign</command>
				<property>f16/avionics/ded-up-down</property>
				<value>-1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>f16/avionics/ded-up-down</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
    
