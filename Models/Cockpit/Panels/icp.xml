    <animation>
        <type>knob</type>
        <object-name>drift</object-name>
        <property>f16/avionics/hud-drift</property>
        <factor>40</factor>
        <offset-deg>-40</offset-deg>
        <drag-direction>vertical</drag-direction>
        <drag-scale-px>10</drag-scale-px><!-- 10 is default -->
        <center>
            <x-m>-0.000831</x-m><y-m>0.01355</y-m><z-m>-0.039324</z-m>
        </center>
        <axis>
            <x>0</x><y>-1</y><z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>f16/avionics/hud-drift</property>
                <factor>1</factor>
                <min>0</min>
                <max>2</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>brt</object-name>
        <property>f16/avionics/hud-brt</property>
        <drag-direction>vertical</drag-direction>
        <drag-scale-px>10</drag-scale-px><!-- 10 is default -->
        <factor>-150</factor>
        <offset-deg>0</offset-deg>
        <center>
            <x-m>  -0.0062 </x-m>
            <y-m>  -0.07268 </y-m>
            <z-m>  -0.02396 </z-m>
        </center>
        <axis>
            <x> 0.0 </x>
            <y> 1.0 </y>
            <z> 0.0 </z>
        </axis>
        <increase>
            <binding>
                <command>property-adjust</command>
                <property>f16/avionics/hud-brt</property>
                <factor>1</factor>
                <step>0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </increase>
        <decrease>
            <binding>
                <command>property-adjust</command>
                <property>f16/avionics/hud-brt</property>
                <factor>1</factor>
                <step>-0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </decrease>
    </animation>
    
