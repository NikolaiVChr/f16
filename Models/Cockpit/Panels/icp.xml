                <command>nasal</command>
                <script>ded.Buttons.wx.doAction();</script>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>button-wx1</tooltip-id>
                <label>DLNK send</label>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>plrt</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>nasal</command>
