   <animation>
        <type>knob</type>
        <object-name>SW_HOOK</object-name>
        <visible>true</visible>
        <property>fdm/jsbsim/systems/hook/tailhook-cmd-norm</property>
        <drag-direction>vertical</drag-direction>
        <drag-scale-px>10</drag-scale-px><!-- 10 is default -->
        <repeatable>true</repeatable>
        <interval-sec>0.75</interval-sec>
        <factor>50</factor>
        <offset-deg>10</offset-deg>
        <roll-deg>-10</roll-deg>
        <heading-deg>7.3</heading-deg>
        <center>
         <x-m>-0.249596</x-m>
         <y-m>-0.281688</y-m>
         <z-m>0.003061</z-m>
        </center>
        <axis>
         <x>0</x>
         <y>1</y>
         <z>0</z>
        </axis>
        <increase>
            <binding>
                <command>property-adjust</command>
                <property>fdm/jsbsim/systems/hook/tailhook-cmd-norm</property>
                <step>-1</step>
                <min>0</min>
