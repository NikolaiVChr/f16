    <animation>
        <type>material</type>
        <object-name>EW_CANVAS_DISPLAY</object-name>
        <condition>
            <greater-than-equals>
                <property>fdm/jsbsim/elec/bus/emergency-dc-1</property>
                <value>20</value>
            </greater-than-equals>
            <property>f16/avionics/ew-disp-switch</property>
          </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </emission>
    </animation>
    
    
    <animation>
        <type>material</type>
        <object-name>gear_lever_light</object-name>
        <condition>
          <greater-than-equals>
                <property>fdm/jsbsim/elec/bus/emergency-dc-1</property>
                <value>20</value>
            </greater-than-equals>
          <not>
            <equals>
                <property>controls/gear/gear-down</property>
                <property>gear/gear[0]/position-norm</property>
            </equals>
          </not>
        </condition>
        <emission>
            <red>1</red>
            <green>0</green>
            <blue>0</blue>
        </emission>
    </animation>

 <animation>
  <type>material</type>
  <object-name>GEAR-nose</object-name>
