<!-- PICK ANIMATIONS -->

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>horn_silencer</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>/controls/gear/gear-horn-cutout</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>/controls/gear/gear-horn-cutout</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>horn_silencer</object-name>
        <factor>-0.0025</factor>
		<property>/controls/gear/gear-horn-cutout</property>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>



   <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>EMER_JETT</object-name>
    <action>
     <button>0</button>
     <repeatable>true</repeatable>
     <binding>
      <command>nasal</command>
      <script>
              setprop("f16/avionics/emer-jett-switch",1);
              if (getprop("fdm/jsbsim/elec/bus/emergency-dc-1") >= 20 and pylons.fcs != nil and (getprop("f16/avionics/gnd-jett") or !getprop("gear/gear[0]/wow"))) {
                pylons.fcs.jettisonFuelAndAG([0,1,7,8]);#drop all fueltanks, dummies and A/G weapons. Except wingtips.
              }
      </script>
     </binding>
    </action>
   </animation>


<animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>alt-gear</object-name>
    <action>
     <button>0</button>
     <repeatable>false</repeatable>
     <binding>
      <command>property-toggle</command>
      <property>/f16/cockpit/alt-gear-handle</property>
     </binding>
    </action>
   </animation>
   <animation>
    <type>translate</type>
    <object-name>alt-gear</object-name>
    <property>/f16/cockpit/alt-gear-handle</property>
    <factor>0.025</factor>
    <axis>
     <x>0</x>
     <y>0</y>
     <z>1</z>
    </axis>
   </animation>
