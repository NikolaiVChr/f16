<!-- PICK ANIMATIONS -->

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>light-land-switch</object-name>
    <object-name>light-land-switch.01</object-name>
    <action>
     <button>0</button>
     <repeatable>true</repeatable>
     <binding>
      <command>nasal</command>
      <script>
                f16_switches.toggleLandlight();
      </script>
     </binding>
    </action>
   </animation>
   
   
   <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>EMER_JETT</object-name>
    <action>
     <button>0</button>
     <repeatable>true</repeatable>
     <binding>
      <command>nasal</command>
      <script>
              setprop("f16/avionics/emer-jett-switch",1);
              if (pylons.fcs != nil and (getprop("f16/avionics/gnd-jett") or !getprop("gear/gear[0]/wow"))) {
                pylons.fcs.jettisonFuelAndAG([0,8]);#drop all fueltanks, dummies and A/G weapons. Except wingtips.
              }
      </script>
     </binding>
    </action>
   </animation>

  <!-- Landing Gear Panel -->

   <animation>
    <type>pick</type>
