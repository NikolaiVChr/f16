<?xml version="1.0"?>

<PropertyList>

 <path>engine.ac</path>

 

 <animation>
  <type>rotate</type>
  <object-name>OIL_needle</object-name>
  <condition>
        <greater-than-equals>
            <property>fdm/jsbsim/elec/bus/emergency-ac-2</property>
            <value>100</value>
        </greater-than-equals>
  </condition>
  <property>engines/engine[0]/oil-pressure-psi</property>
  <factor>3.3</factor>
  <center>
   <x-m>-0.48824</x-m>
   <y-m>0.26706</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>-1.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NOZ_POS_needle</object-name>
  <condition>
        <greater-than-equals>
            <property>fdm/jsbsim/elec/bus/emergency-ac-2</property>
            <value>100</value>
        </greater-than-equals>
  </condition>
  <property>engines/engine[0]/nozzle-pos-norm</property>
  <factor>250.0</factor>
  <center>
   <x-m>-0.44984</x-m>
   <y-m>0.27188</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>-1.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RPM_needle</object-name>
  <condition>
        <greater-than-equals>
            <property>fdm/jsbsim/elec/bus/batt-1</property>
            <value>20</value>
        </greater-than-equals>
  </condition>
  <property>engines/engine[0]/n2</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
   <entry>
    <ind>60.0</ind>
    <dep>122.0</dep>
   </entry>
   <entry>
    <ind>100.0</ind>
    <dep>328.0</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>-0.39945</x-m>
   <y-m>0.27593</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>-1.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type> <!-- 2192 --> <!-- 17.5 graden -->
  <object-name>FTIT_needle</object-name>
  <condition>
        <greater-than-equals>
            <property>fdm/jsbsim/elec/bus/batt-1</property>
            <value>20</value>
        </greater-than-equals>
  </condition>
  <property>engines/engine[0]/ftit-degc</property>
  <offset-deg>65</offset-deg>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0.0</dep>
   </entry>
   <entry>
    <ind>200</ind>
    <dep>18.5</dep>
   </entry>
   <entry>
    <ind>700</ind>
    <dep>117.5</dep>
   </entry>
   <entry>
    <ind>800</ind>
    <dep>177.5</dep>
   </entry>
   <entry>
    <ind>900</ind>
    <dep>237.5</dep>
   </entry>
   <entry>
    <ind>1000</ind>
    <dep>297.5</dep>
   </entry>
   <entry>
    <ind>1200</ind>
    <dep>337.5</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>-0.3431</x-m>
   <y-m>0.29056</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>-1.0</z>
  </axis>
 </animation>

 <!-- lighting -->
 <animation>
  <object-name>OIL_disk</object-name>
  <object-name>OIL_needle</object-name>
  <type>material</type>
  <emission>
   <factor-prop>controls/lighting/lighting-panel/pri-inst-pnl</factor-prop>
   <red>0.6</red>
   <green>0.4</green>
   <blue>0.3</blue>
  </emission>
 </animation>

 <animation>
  <object-name>NOZ_POS_disk</object-name>
  <object-name>NOZ_POS_needle</object-name>
  <type>material</type>
  <emission>
   <factor-prop>controls/lighting/lighting-panel/pri-inst-pnl</factor-prop>
   <red>0.6</red>
   <green>0.4</green>
   <blue>0.3</blue>
  </emission>
 </animation>

 <animation>
  <object-name>RPM_disk</object-name>
  <object-name>RPM_needle</object-name>
  <type>material</type>
  <emission>
   <factor-prop>controls/lighting/lighting-panel/pri-inst-pnl</factor-prop>
   <red>0.6</red>
   <green>0.4</green>
   <blue>0.3</blue>
  </emission>
 </animation>

 <animation>
  <object-name>FTIT_disk</object-name>
  <object-name>FTIT_needle</object-name>
  <type>material</type>
  <emission>
   <factor-prop>controls/lighting/lighting-panel/pri-inst-pnl</factor-prop>
   <red>0.6</red>
   <green>0.4</green>
   <blue>0.3</blue>
  </emission>
 </animation>

</PropertyList>
