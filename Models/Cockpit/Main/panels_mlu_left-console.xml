      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/flight/flaps</property>
      </binding>
    </action>
   </animation> 
      
   <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>SW_JET_FUEL</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-cycle</command>
        <property>f16/engine/jet-fuel</property>
        <value type="int">-1</value>
        <value type="int">0</value>
        <value type="int">1</value>
        <wrap>true</wrap>
      </binding>
    </action>
   </animation> 
   
   <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>SW_MPO</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/fdm/jsbsim/fcs/fbw-override</property>
      </binding>
    </action>
   </animation> 
   
   <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>SW_MAIN_POWER</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-cycle</command>
        <property>fdm/jsbsim/elec/switches/main-pwr</property>
        <value type="int">0</value>
        <value type="int">1</value>
        <value type="int">2</value>
        <wrap>true</wrap>
      </binding>
    </action>
   </animation>
   
   <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>SW_AIR_REFUEL</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>systems/refuel/serviceable</property>
      </binding>
    </action>
   </animation> 
   
   <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>SW_EPU</object-name>
    <object-name>SW_EPU_COVER</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-cycle</command>
        <property>fdm/jsbsim/elec/switches/epu</property>
        <value type="int">0</value>
        <value type="int">1</value>
        <value type="int">2</value>
        <wrap>true</wrap>
      </binding>
      <binding>
        <command>nasal</command>
        <script>
          if (getprop("fdm/jsbsim/elec/switches/epu")==1) {
            setprop("fdm/jsbsim/elec/switches/epu-cover",0);
          } else {
            setprop("fdm/jsbsim/elec/switches/epu-cover",1);
          }
        </script>
      </binding>
    </action>
   </animation>
   
   <animation>
