<?xml version="1.0"?>

<PropertyList>

 <path>panels_mlu_right-console.ac</path>

    <animation>
        <type>knob</type>
        <object-name>inst-pnl-flood-knob</object-name>
        <property>controls/lighting/lighting-panel/flood-inst-pnl</property>
        <offset-deg>135</offset-deg>
        <factor>-222</factor>
        <axis>
            <object-name>inst-pnl-flood-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/flood-inst-pnl</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>inst-pnl-primary-knob</object-name>
        <property>controls/lighting/lighting-panel/pri-inst-pnl</property>
        <offset-deg>135</offset-deg>
        <factor>-222</factor>
        <axis>
            <object-name>inst-pnl-primary-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/pri-inst-pnl</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>console-flood-knob</object-name>
        <property>controls/lighting/lighting-panel/console-flood</property>
        <offset-deg>155</offset-deg>
        <factor>-330</factor>
        <axis>
            <object-name>console-flood-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/console-flood</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>data-entry-display-knob</object-name>
        <property>controls/lighting/lighting-panel/data-entry-display</property>
        <offset-deg>135</offset-deg>
        <factor>-222</factor>
        <axis>
            <object-name>data-entry-display-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/data-entry-display</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>console-primary-knob</object-name>
        <property>controls/lighting/lighting-panel/console-primary</property>
        <offset-deg>135</offset-deg>
        <factor>-222</factor>
        <axis>
            <object-name>console-primary-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/console-primary</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>material</type>
        <object-name>RIGHT_CONSOLE</object-name>
        <emission>
            <factor-prop>controls/lighting/lighting-panel/flood-inst-pnl</factor-prop>
            <red>0.45</red>
            <green>0.34</green>
            <blue>0.18</blue>
        </emission>
    </animation>
    <effect>
        <name>models-effects-f16panel-left</name>
        <inherits-from>Models/Effects/f16panel-left</inherits-from>
        <object-name>LIGHTING_1</object-name>
    </effect>

</PropertyList>
