 <effect>
  
  <inherits-from>Effects/glass</inherits-from>
  
  <object-name>HUDImage10</object-name>
  <parameters>
    <metallic-factor type="float">1.0</metallic-factor>
    <roughness-factor type="float">1.0</roughness-factor>
    <alpha-cutoff>-1.0</alpha-cutoff>
    <blend>1</blend>
  </parameters>
 </effect>

 <animation>
  <object-name>HUDImage10</object-name>
  <type>material</type>
  <emission>
   <red>100</red>
   <green>100</green>
   <blue>100</blue>
  </emission>
 </animation>

