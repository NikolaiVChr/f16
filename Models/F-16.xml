   <z>0.201098</z>
  </axis>
 </animation>

  <!-- spinning wheel calc

     radius = 1.80323 - 1.51444 = 0.28879
     circum = 2*pi*radius = 1.8145
     factor = 1/circum * 60 = 0.6318 * 60 = 33.0666

     -->
<animation>
    <type>spin</type>
    <object-name>LeftMainTire</object-name>
    <condition>
        <property>gear/gear[2]/wow</property>
    </condition>
    <property>gear/gear[2]/rollspeed-ms</property><!-- gear/gear[0]/rollspeed-ms -->
    <factor>-33.0666</factor>
    <axis>   
     <x1-m>0.563844</x1-m>
     <y1-m>-1.27316</y1-m>
     <z1-m>-1.51444</z1-m>
     <x2-m>  0.562878 </x2-m>
     <y2-m> -1.1289 </y2-m>
     <z2-m> -1.48814 </z2-m>
  </axis>
</animation>

