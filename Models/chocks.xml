<?xml version="1.0"?>

<PropertyList>

<!--CHOCKS -->
<animation>
 <type>select</type>
 <object-name>chocks</object-name>
 <condition>
   <property>controls/gear/brake-parking</property>
   <not><property>controls/lighting/ext-lighting-panel/anti-collision</property></not>
 </condition>
</animation>
 
<!-- NOSE CHOCKS -->
<model>
