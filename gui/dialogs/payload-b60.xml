<?xml version="1.0" encoding="iso-8859-1"?>

<PropertyList>
  
  <name>payload-b60</name>
  <modal type="bool">false</modal>
  <draggable>true</draggable>
  <resizable>true</resizable>
  <width>800</width>
  <layout>vbox</layout>
  
  <group>
    <layout>hbox</layout>
    <empty>
      <stretch>1</stretch>
    </empty>
    
    <text>
      <label>Payload presets</label>
    </text>
    
    <empty>
      <stretch>1</stretch>
    </empty>
    
    <button>
      <pref-width>16</pref-width>
      <pref-height>16</pref-height>
      <legend></legend>
      
      <default>1</default>
      <keynum>27</keynum>
      <border>2</border>
      <binding>
        <command>dialog-close</command>
      </binding>
      <binding>
        <command>dialog-apply</command>
      </binding>
    </button>
  </group>

  <hrule/>

  <group>
    <layout>hbox</layout>
    <button>
        <legend>Default load</legend>
        <binding>
          <command>nasal</command>
          <script>pylons.default();</script>
        </binding>
    </button>

    <vrule/>

    <button>
        <legend>Refuel all tanks</legend>
        <binding>
          <command>nasal</command>
          <script>pylons.refuel();</script>
        </binding>
    </button>

    <vrule/>

    <button>
        <legend>Rearm 20mm cannon</legend>
        <binding>
          <command>nasal</command>
          <script>pylons.cannon_load();</script>
        </binding>
    </button>

  <vrule/>

    <button>
        <legend>Clean load</legend>
        <binding>
          <command>nasal</command>
          <script>pylons.clean();</script>
        </binding>
    </button>
  </group>

